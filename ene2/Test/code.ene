namespace data
{
    c32°: l;
}

struct point
{
    c32°: x;
    c32°: y;
    
    c32 length(c32° v)
    {
       return v~1+; 
    }
}

asm("global main");
asm("extern printf");
void main()
{
    point: p;
    p.x = 64;
    
    data::l    c8° = p.x~;
    data::l 1+ c8° = p.length(64);
    data::l 2+ c8° = p.length(65);
    data::l 3+ c8° = 0;
    
    io::o::print(data::l, 3);
};

namespace io
{
    namespace o
    {
        void print(c8° string, c8 count)
        {   
            while (count~)
            {
                io::print_str(string);
                math::dec(count);
            }
        };
    }
    
    void print_str(c8°° string)
    {
        string~~ asm("call printf") ^;
    };
}

namespace math
{
    void inc(c32°° v_ptr)
    {
        v_ptr~~ v_ptr~ = asm("inc dword [esp]");
    };

    void dec(c32°° v_ptr)
    {
        v_ptr~~ v_ptr~ = asm("dec dword [esp]");
    };
}